<div class="container">
  <h1><%= @password.name %></h1>

  <p>
    <strong>Key:</strong>
    <%= @password.key %>
  </p>

  <p>
    <strong>Team:</strong>
    <%= Team.find(@password.team_id).name if @password.team_id %>
  </p>

  <%= link_to 'Edit', edit_password_path(@password) %> |
  <%= link_to 'Back', passwords_path %>

</div>
